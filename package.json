{
  "name": "voidtxt",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "clean": "cargo clean --manifest-path src-tauri/Cargo.toml",
    "android-share": "cd \"$(npm prefix)\" && qrshare src-tauri/gen/android/app/build/outputs/apk/universal/release/app-universal-release.apk",
    "android-build": "pnpm tauri android build --apk true",
    "android-build-share": "pnpm tauri android build --apk true && pnpm android-share",
    "android-clean-build": "pnpm clean && pnpm android-build",
    "android-open": "pnpm tauri android dev --open",
    "android-build-debug": "pnpm tauri android build --debug"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.18",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "~2",
    "@tauri-apps/plugin-fs": "~2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-os": "~2.3.2",
    "lucide-react": "^0.562.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.10.1",
    "tauri-plugin-app-exit-api": "~0.1.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
